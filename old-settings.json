{
  "editor.minimap.enabled": false,
  "editor.wrappingIndent": "indent",
  "editor.overviewRulerBorder": false,
  "editor.lineHeight": 24,
  "editor.fontFamily": "iA Writer Duospace, monospace",
  "editor.fontSize": 14,
  "editorCursor.foreground": "#F6B762",    
  "terminalCursor.foreground": "#A0F55D",
  "editor.lineHighlightBackground": "#2C2C27",
  "workbench.colorTheme": "Default Dark+",
  "terminal.integrated.cwd": "/Users/igw/Library/Mobile Documents/com~apple~CloudDocs/Papers",
  "[markdown]": {
    "editor.quickSuggestions": true
  },
  "git.enableSmartCommit": true,
  "git.postCommitCommand": "sync",
  "files.exclude": {
    "**/node_modules": true
  },
  "files.watcherExclude": {
    "**/node_modules": true
  },
  "mdmath.macros": {
    "\\Land": "\\bigwedge",
    "\\Lor" : "\\bigvee",
    "\\es" : "\\emptyset",
    "\\imp" : "\\Rightarrow",
    "\\iff" : "\\Leftrightarrow",
    "\\sat" : "\\vDash",
    "\\notsat" : "\\nvDash",
    "\\incl" : "\\subseteq",
    "\\fleq" : "\\preccurlyeq",
    "\\fgeq" : "\\succcurlyeq",
    "\\fle" : "\\prec",
    "\\bleq" : "\\sqsubseteq",
    "\\ble" : "\\sqsubset",
    "\\ttrue" : "\\mathit{tt}",
    "\\ffalse" : "\\mathit{ff}",
    "\\Inf" : "\\mathrm{Inf}",
    "\\set" : "\\{#1\\}",
    "\\a": "\\alpha",
    "\\b": "\\beta",
    "\\d" : "\\delta",
    "\\e" : "\\varepsilon",
    "\\f" : "\\varphi",
    "\\g" : "\\gamma",
    "\\l" : "\\lambda",
    "\\k" : "\\kappa",
    "\\m" : "\\mu",
    "\\n" : "\\nu",
    "\\p" : "\\psi",
    "\\r" : "\\rho",
    "\\s" : "\\sigma",
    "\\vs" : "\\varsigma",
    "\\t" : "\\tau",
    "\\th" : "\\theta", 
    "\\vth" : "\\vartheta",
    "\\u" : "\\upsilon",
    "\\v" : "\\varsigma",
    "\\w" : "\\omega",
    "\\x" : "\\xi",
    "\\z" : "\\zeta",
    "\\D" :"\\Delta",
    "\\F" : "\\Phi",
    "\\G" : "\\Gamma",
    "\\P" : "\\Psi",
    "\\T" : "\\theta",
    "\\W" : "\\Omega",
    "\\S" : "\\Sigma",
    "\\Aa" : "\\mathcal{A}",
    "\\Bb" : "\\mathcal{B}",
    "\\Cc" : "\\mathcal{C}",
    "\\Dd" : "\\mathcal{D}",
    "\\Ee" : "\\mathcal{E}",
    "\\Ff" : "\\mathcal{F}",
    "\\Gg" : "\\mathcal{G}",
    "\\Hh" : "\\mathcal{H}",
    "\\Ii" : "\\mathcal{I}",
    "\\Jj" : "\\mathcal{J}",
    "\\Kk" : "\\mathcal{K}",
    "\\Ll" : "\\mathcal{L}",
    "\\Mm" : "\\mathcal{M}",
    "\\Nn" : "\\mathcal{N}",
    "\\Oo" : "\\mathcal{O}",
    "\\Pp" : "\\mathcal{P}",
    "\\Qq" : "\\mathcal{Q}",
    "\\Rr" : "\\mathcal{R}",
    "\\Ss" : "\\mathcal{S}",
    "\\Tt" : "\\mathcal{T}",
    "\\Uu" : "\\mathcal{U}",
    "\\Vv" : "\\mathcal{V}",
    "\\Ww" : "\\mathcal{W}",
    "\\Yy" : "\\mathcal{Y}",
    "\\Zz" : "\\mathcal{Z}",
    "\\struct" : "\\langle #1 \\rangle",
    "\\wh" : "\\widehat{#1}",
    "\\wt" : "\\widetilde",
    "\\Sem" : "\\parallel #1 \\parallel^{#3}_{#2}",
    "\\sem" : "[\\![ #1 ]\\!]",
    "\\sq" : "[#1]",
    "\\di" : "\\langle #1 \\rangle",
    "\bar" : "\\overline{#1}", 
    "\\sleq" : "\\sqsubseteq",
    "\\act" : "\\stackrel{#1}{\\longrightarrow}"
}, 
  "vscodeMarkdownNotes.noteCompletionConvention": "noExtension",
  "command-runner.terminal.name": "runCommand",
  "command-runner.terminal.autoClear": true,
  "command-runner.terminal.autoFocus": true,
  "command-runner.commands": {
      "open PDF" : "open \"${selectedText}\"" ,
      "echo workspaceFolder": "echo ${workspaceFolder}",
        "echo file": "echo ${file}"}
}

